    "def efficient_cliffs_delta(a, b):\n",
    "    a = np.sort(a)\n",
    "    b = np.sort(b)\n",
    "    m, n = len(a), len(b)\n",
    "    more = sum(bisect_right(b, x) for x in a)\n",
    "    less = sum(n - bisect_left(b, x) for x in a)\n",
    "    delta = (more - less) / (m * n)\n",
    "    return delta"
   ],
